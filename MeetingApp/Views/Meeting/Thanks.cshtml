@model UserInfo

@{
    ViewBag.Title = "Başvuru Özeti";

}



<main class="text-center">

    @if(Model.WillAttend == true)
    {
        <p>Teşekkürler, @Model.Name toplantıya sizinle birlikte @ViewBag.UserCount kişi katılıor</p>
        <a class="btn btn-sm btn-outline-primary" href="/meeting/list">Katilimcilari Listele</a>

    } else
    {
        <p>@Model.Name , katılmadığın için üzgünüz</p>
        <a class="btn btn-sm btn-outline-primary" href="/meeting/apply">Başvuru Formu</a>

    }




</main>